APP_NAME="ApiSMS Gateway"
APP_ENV=local
APP_KEY=
APP_DEBUG=true
APP_URL=http://apisms.test
APP_TIMEZONE=Africa/Djibouti

APP_LOCALE=en
APP_FALLBACK_LOCALE=en
APP_FAKER_LOCALE=en_US

APP_MAINTENANCE_DRIVER=file

PHP_CLI_SERVER_WORKERS=4

BCRYPT_ROUNDS=12

# Logging Configuration
LOG_CHANNEL=stack
LOG_STACK=single,json
LOG_DEPRECATIONS_CHANNEL=null
LOG_LEVEL=debug
LOG_SLACK_WEBHOOK_URL=
LOG_JSON_ENABLED=false

DB_CONNECTION=mysql
DB_HOST=127.0.0.1
DB_PORT=3306
DB_DATABASE=apisms
DB_USERNAME=root
DB_PASSWORD=

# Kannel SMS Gateway Configuration
KANNEL_URL=http://your-kannel-server:13013/cgi-bin/sendsms
KANNEL_USERNAME=your_username
KANNEL_PASSWORD=your_password
KANNEL_FROM=+253XXXXXXXX
KANNEL_TIMEOUT=30
KANNEL_RETRY_ATTEMPTS=3
KANNEL_RETRY_DELAY=2

# SMS Configuration
SMS_DEFAULT_COUNTRY_CODE=+253
SMS_RATE_LIMIT_PER_MINUTE=60
SMS_MAX_LENGTH=160

SESSION_DRIVER=database
SESSION_LIFETIME=120
SESSION_ENCRYPT=false
SESSION_PATH=/
SESSION_DOMAIN=null

BROADCAST_CONNECTION=log
FILESYSTEM_DISK=local
QUEUE_CONNECTION=database

# Queue Configuration
QUEUE_FAILED_DRIVER=database
QUEUE_RETRY_AFTER=300
QUEUE_MAX_EXCEPTIONS=3

# Cache Configuration
CACHE_STORE=redis
CACHE_PREFIX=apisms_cache
CACHE_TTL=3600

MEMCACHED_HOST=127.0.0.1

# Redis Configuration
REDIS_CLIENT=phpredis
REDIS_HOST=127.0.0.1
REDIS_PASSWORD=null
REDIS_PORT=6379
REDIS_DB=0
REDIS_CACHE_DB=1
REDIS_SESSION_DB=2

# Security Configuration
API_KEY_ENCRYPTION_KEY=
WEBHOOK_IP_WHITELIST="127.0.0.1,::1"
CORS_ALLOWED_ORIGINS="*"
CORS_ALLOWED_METHODS="GET,POST,PUT,DELETE,OPTIONS"
CSRF_PROTECTION_ENABLED=true
HTTPS_ONLY=false
FORCE_HTTPS=false

# Monitoring Configuration  
MONITORING_ENABLED=true
PROMETHEUS_METRICS_ENABLED=false
UPTIME_MONITORING_URL=
HEALTH_CHECK_TOKEN=

# Backup Configuration
DB_BACKUP_ENABLED=true
DB_BACKUP_SCHEDULE="0 2 * * *"
DB_BACKUP_RETENTION_DAYS=30
DB_BACKUP_STORAGE_DISK=local

# Performance Configuration
RESPONSE_COMPRESSION_ENABLED=true
EAGER_LOADING_ENABLED=true
DB_QUERY_LOG_ENABLED=false

MAIL_MAILER=log
MAIL_SCHEME=null
MAIL_HOST=127.0.0.1
MAIL_PORT=2525
MAIL_USERNAME=null
MAIL_PASSWORD=null
MAIL_FROM_ADDRESS="hello@example.com"
MAIL_FROM_NAME="${APP_NAME}"

AWS_ACCESS_KEY_ID=
AWS_SECRET_ACCESS_KEY=
AWS_DEFAULT_REGION=us-east-1
AWS_BUCKET=
AWS_USE_PATH_STYLE_ENDPOINT=false

VITE_APP_NAME="${APP_NAME}"
